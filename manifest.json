{
  "manifest_version": 2,

  "name": "GitHub Superfilter",
  "version": "1.0.1",
  "description": "Adds a lovely filter to GitHub pull requests",

  "developer": {
    "name": "Tomer Ron",
    "url": "https://github.com/TomerRon"
  },

  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["bundle.js"]
    }
  ],

  "icons": {
    "48": "assets/icon.png",
    "96": "assets/icon@2x.png"
  }
}
